packadd! dracula
syntax enable
colorscheme dracula

" setting
set fenc=utf-8

set nobackup

set noswapfile

set autoread

set hidden

" visual
set number

set smartindent

set visualbell

set showmatch

syntax enable

color dracula

" tab
set expandtab

set tabstop=2

set shiftwidth=2

" search
set ignorecase

set smartcase

set incsearch

set wrapscan

set hlsearch
